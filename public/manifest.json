{
  "version": "0.0.0",
  "manifest_version": 2,
  "minimum_chrome_version": "56.0.0",
  "name": "downloader",
  "short_name": "XX downloader",
  "description": "XX 下载器",
  "offline_enabled": true,
  "default_locale": "zh_CN",
  "icons": {},
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": {},
    "default_popup": "popup.html",
    "default_title": "downloader"
  },
  "permissions": [
    "tabs",
    "webRequest",
    "notifications",
    "*://*.baidu.com/*",
    "*://fundgz.1234567.com.cn/*"
  ],
  "web_accessible_resources": [
    "static/js/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}