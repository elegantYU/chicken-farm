{
  "version": "0.0.0",
  "manifest_version": 2,
  "minimum_chrome_version": "56.0.0",
  "name": "养基场-基金管理助手",
  "short_name": "基金管理助手",
  "description": "基金管理助手",
  "offline_enabled": true,
  "default_locale": "zh_CN",
  "icons": {
    "16": "static/icons/logo.png",
    "32": "static/icons/logo.png",
    "128": "static/icons/logo.png"
  },
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": {},
    "default_popup": "popup.html",
    "default_title": "downloader"
  },
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "notifications",
    "*://*.baidu.com/*",
    "*://*.10jqka.com.cn/*",
    "*://fundgz.1234567.com.cn/*",
    "*://*.eastmoney.com/*",
    "*://api.fund.eastmoney.com/*"
  ],
  "web_accessible_resources": [
    "static/js/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
